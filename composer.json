		"Mouf\\Security\\" : "tests/"
  	  }
    },
	"minimum-stability" : "dev",
    "prefer-stable": true,
	"extra" : {
		"mouf" : {
			"install" : [{
					"type" : "class",
					"class" : "Mouf\\Security\\DAO\\TablesInstaller",
					"description" : "The install process of security.daos.tdbm creates patches creating the user/role/right tables."
				}
			],
			"logo" : "icon.png"
		}
	}
}
